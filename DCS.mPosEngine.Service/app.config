<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net" requirePermission="false" />
  </configSections>

  <appSettings>
    <add key="TestValue" value="test" />
  </appSettings>

  <system.diagnostics>
    <sources>
      <source name="System.ServiceModel" switchValue="Error, Critical" propagateActivity="true">
        <listeners>
          <add name="traceListener" type="System.Diagnostics.XmlWriterTraceListener" initializeData="c:\log\Traces.svclog" />
        </listeners>
      </source>
    </sources>
  </system.diagnostics>
  
   <log4net debug="false">
     <appender name="EventLogAppender" type="log4net.Appender.EventLogAppender">
       <applicationName value="DCS mPosEngine" />
       <layout type="log4net.Layout.PatternLayout">
         <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
       </layout>
     </appender>
     
    <appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="${NW}\\dcs\\data\\logs\\" />
      <rollingStyle value="Date" />
      <DatePattern value="yyyyMMdd'\\mPosEngine_${COMPUTERNAME}.log'" />
      <param name="staticLogFileName" value="false" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date{yyyy/MM/dd HH:mm:ss} %-5level (%logger) %message %newline" />
      </layout>
    </appender>

     <logger name="DCS.ConfigEngine">
       <level value="OFF" />
     </logger>
     
     <logger name="DCS">
       <level value="ALL" />
       <appender-ref ref="EventLogAppender" />
       <appender-ref ref="RollingLogFileAppender" />
     </logger>

     <!-- <logger name="org.hibernate.SQL" level="DEBUG">
       <appender-ref ref="RollingLogFileAppender" />
     </logger>

    <logger name="org.hibernate.type" level="TRACE">
      <appender-ref ref="RollingLogFileAppender" />
    </logger>

    <logger name="org.hibernate.tool.hbm2ddl" level="DEBUG">
      <appender-ref ref="RollingLogFileAppender" />
    </logger>-->

     <!--<logger name="NHibernate">
       <level value="ALL"/>
       <appender-ref ref="RollingLogFileAppender"/>
     </logger>-->

     <root>      
			<level value="ALL" />
			<!--<appender-ref ref="RollingLogFileAppender"/>-->
		</root>
  </log4net>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="NHibernate" publicKeyToken="aa95f207798dfdb4" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-5.0.0.0" newVersion="5.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
