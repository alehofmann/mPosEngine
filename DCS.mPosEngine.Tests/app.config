<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net" />
  </configSections>

  <appSettings>
    <add key="TestValue" value="test" />
  </appSettings>

  <log4net debug="false">
    <appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="${NW}\\dcs\\data\\logs\\" />
      <rollingStyle value="Date" />
      <DatePattern value="yyyyMMdd'\\mPosEngine_${COMPUTERNAME}.log'" />
      <param name="staticLogFileName" value="false" />
      <layout type="log4net.Layout.PatternLayout">

        <header value="[Header] " />
        <footer value="[Footer] " />
        <conversionPattern value="%date{yyyy/MM/dd HH:mm:ss} %-5level (%logger) %message %newline" />
      </layout>
    </appender>

    <!-- ### log just the SQL ### -->
    <logger name="org.hibernate.SQL" level="DEBUG">
      <appender-ref ref="RollingLogFileAppender" />
    </logger>

    <!-- ### log JDBC bind parameters ### -->
    <!--<logger name="org.hibernate.type" level="TRACE"/>-->

    <!--<logger name="org.hibernate.tool.hbm2ddl" level="DEBUG"/>-->

    <root level="ALL">
      <appender-ref ref="RollingLogFileAppender" />
    </root>
    
    <!-- <root>      
			<level value="ALL" />
			<appender-ref ref="RollingLogFileAppender" />
		</root>-->

    <logger name="DCS.ConfigEngine">
      <level value="OFF" />
    </logger>

    <logger name="DCS">
      <level value="ALL" />
      <appender-ref ref="EventLogAppender" />
      <appender-ref ref="RollingLogFileAppender" />
    </logger>
    
  </log4net>
  
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" /></startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="NHibernate" publicKeyToken="aa95f207798dfdb4" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-5.0.0.0" newVersion="5.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
